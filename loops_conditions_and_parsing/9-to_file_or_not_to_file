#!/usr/bin/env bash  
#A script that gives information about the school file 
if [ -e "school" ]
then echo "school file exists"
elif [ -f "school" ]
then echo "school is a regular file"
else echo "school file does not exist"
fi
if [ -s "school" ]
then echo "school file is empty"
else echo "school file is not empty"
fi
