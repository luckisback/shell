#!/usr/bin/env bash  
#A script that gives information about the school file 
File="school"
if [ -f $File ]
then echo "$File is a regular file"
elif [ -s $File ]
then echo "$File file is not empty"
else echo "$File file is empty"
fi
if [ -e $File ]
then echo "$File file exists"
else echo "$File file does not exist"
fi
