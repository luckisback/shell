#!/usr/bin/env bash
# Displaying number with an insert of eords
nbre=1
while ((nbre != 101))
do 
	echo "$nbre"
	if [ nbre != 101 ]
	then 
	if  [ $((nbre%5))  = 0 ] && [ $((nbre%3)) = 0 ]
	then echo "FizzBuzz"
	fi
	
	if  [ $((nbre%3))  = 0 ]
	then echo "Fizz"
	fi

	if  [ $((nbre%5))  = 0 ]
	then echo "Buzz"
	fi
	((nbre++))
	fi

	((nbre++))

done
